<h3> <%= @item.title%> </h>


<pre class= "preStyle" > <%= raw(@item.content)%> </pre>


<%= link_to 'Archive',archive_sessions_path(id: @item.item_id) %>

<%- if @item.favorite == 0 %>
          <%= link_to 'Favorite',favorite_sec_sessions_path(given_url: @item.url, item_id: @item.item_id) %>
        <%- else %>
          <%= link_to 'unFavorite',unfavorite_sec_sessions_path(given_url: @item.url,item_id: @item.item_id) %>
<%- end %>